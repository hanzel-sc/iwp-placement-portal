{
  "rewrites": [
    {
      "source": "/",
      "destination": "/fronend/auth.html"
    },
    {
      "source": "/home",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/main",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/company/company-dashboard.html",
      "destination": "/frontend/company/company-dashboard.html"
    },
    {
      "source": "/company/company-profile.html", 
      "destination": "/frontend/company/company-profile.html"
    },
    {
      "source": "/company/create-job.html",
      "destination": "/frontend/company/create-job.html"
    },
    {
      "source": "/company/view-applications.html",
      "destination": "/frontend/company/view-applications.html"
    },
    {
      "source": "/js/(.*)",
      "destination": "/frontend/js/$1"
    },
    {
      "source": "/css/(.*)",
      "destination": "/frontend/css/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/frontend/images/$1"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": false
}
